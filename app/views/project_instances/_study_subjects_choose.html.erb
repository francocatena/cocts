<% i18n_scope = [:projects, :sociodemographic_forms, :study_subjects_choose, :options] %>
<% subjects_choose = [] %>

<% STUDY_SUBJECTS_CHOOSE.each do |study| %>
  <% subjects_choose << [t(study, :scope => i18n_scope), study] %>
<% end %>

<%= f.input :study_subjects_choose, collection: subjects_choose, as: :radio_buttons,
  checked: @project_instance.study_subjects_choose %>

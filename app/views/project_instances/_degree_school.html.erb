<% i18n_scope = [:projects, :sociodemographic_forms, :degree_school, :options] %>
<% degrees = [] %>

<% DEGREES_SCHOOL.each do |degree| %>
  <% degrees << [t(degree, :scope => i18n_scope), degree] %>
<% end %>

<%= f.input :degree_school, collection: degrees, as: :radio_buttons,
  checked: @project_instance.degree_school %>
